        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// ANIMATION and MOVEMENT
event_inherited();
update_sprite();

// ANIMATE IF WALKING
if (global.isometricView) {
    animate();
}

// FALLING
if (FALLING) {
    image_xscale -= delta(.1);
    image_yscale -= delta(.01);
    z -= delta(1);
    if (image_xscale &lt; 0) then image_xscale = 0;
    if (image_yscale &lt; 0) then image_yscale = 0;
    waitTime = 100; // keep waiting while we fall
    fallTime--;
    if (fallTime &lt;= 0) {
        alarm[1] = 1;
    }
} else {

    if (waitTime &gt; 0) {
        waitTime--;
    }
    
    // MOVEMENT
    if (MOVING) {
        CAMERA.target = id;
        CURSOR.MODE = MODES.waiting;
        get_z();
        getCellFromCoordsPrecise(x, y);  // allows drawing of the sprite while in motion
        
        var finishedPath = false;
        
        // FINISHED MOVING
        if ( (movementPoints &lt; 0) 
        or ( current_path_node == pathLength) ) {
            finishedPath = true;
        }
        
        // NOT FINISHED MOVING
        if (!finishedPath) {
            // move towards the node
            walk_towards_point(pathfinding_targX, pathfinding_targY, delta(global.movementSpeed));
            update_sprite();
            
            // on each node reached
            if ((x == pathfinding_targX) and (y == pathfinding_targY)) {
                show_debug_message("arrived at a node, moving to the next");
                pathfinding_next_node();
                movementPoints--;
            }
        }
        
        
        // END
        if (finishedPath) {
            MOVING = false;
            
            if !(object_is_ancestor(object_index, NPC)) {
                column = targetColumn;
                row = targetRow;
                CURSOR.MODE = MODES.free;                                           // return control to the cursor
                MOVING = false;
                updateGrid(id, sourceColumn, sourceRow, targetColumn, targetRow);   // empty old coords and add id to new position.
            } else {
                updateGrid(id, sourceColumn, sourceRow, column, row);               // empty old coords and add id to new position.
            }
        }
    }
}
</string>
